{
  "name": "sw_jewelry",
  "version": "1.2.0",
  "description": "e-commerce site for sharon wisely jewelry",
  "keywords": [
    "react",
    "express",
    "mongodb",
    "node"
  ],
  "repository": {
    "type": "git",
    "url": "tbd"
  },
  "license": "ISC",
  "author": "Lincoln Wisely",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "concurrently \"pnpm run dev:server\" \"pnpm run dev:client\"",
    "dev:server": "pnpm --filter server dev",
    "dev:client": "pnpm --filter client start",
    "build": "pnpm --filter client build",
    "start": "pnpm run start --filter server",
    "seed": "pnpm run seed --filter server",
    "setup-users": "pnpm run setup-users --filter server",
    "setup": "pnpm run seed --filter server && pnpm run setup-users --filter server",
    "install:all": "pnpm install",
    "clean": "pnpm run clean:root && pnpm run clean:client && pnpm run clean:server",
    "clean:root": "rm -rf node_modules pnpm-lock.yaml",
    "clean:client": "rm -rf client/node_modules client/pnpm-lock.yaml",
    "clean:server": "rm -rf server/node_modules server/pnpm-lock.yaml"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@9.0.0",
  "dependencies": {
    "@types/react": "^19.1.9"
  }
}
